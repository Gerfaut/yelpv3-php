{
    "name": "gerfaut/yelpv3-php",
    "description": "A php client for consuming Yelp API v3 (Fusion)",
    "homepage" : "https://github.com/Gerfaut/yelpv3-php",
    "authors" : [
        {
            "name" : "Christophe Leemans",
            "email" : "hello.gerfaut@gmail.com",
            "role" : "Developer"
        }
    ],
    "keywords": [
        "api",
        "oauth1",
        "yelp",
        "fusion",
        "php"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "jms/serializer": "^1.4",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Gerfaut\\Yelp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gerfaut\\Yelp\\Test\\": "tests/src/"
        }
    },
    "minimum-stability": "stable"
}
